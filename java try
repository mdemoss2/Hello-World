#java
public class Math {
  //setup some basic mathematic methods.
  //add
  public int add(int a, int b) {
    return a + b;
  }
  
  //subtract
  public int subtract(int a, int b) {
    //subtract the first variable by the second
    return a - b;
  }
  
  //multiply
  public int multiply(int a, int b) {
    return a * b;
  }
  
  //divide
  public int divide(int a, int b) {
    return a / b;
  }
  
  //exponent
  public int exponent(int base, int exponent) {
    int total = 1;
    for (int i = 0; i < exponent; i++) {
      total *= base;
    }
    return total;
  }
  
  //factorial
  public int factorial(int a) {
    int total = 1;
    for (int i = a; i > 0; i--) {
      total *= i;
    }
    return total;
  }
  
  
}



